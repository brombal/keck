{
  "name": "keck",
  "version": "0.0.2",
  "description": "Proxy-style state observations for React and vanilla",
  "author": "Alex Brombal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brombal/keck.git"
  },
  "scripts": {
    "start": "parcel example/index.html --dist-dir ./example-dist",
    "build": "rm -rf dist && parcel build && npm run compress-benchmark",
    "compress-benchmark": "gzip --best -k dist/compressed-benchmark.js",
    "test": "jest --coverage"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types.d.ts",
  "compressed-benchmark": "./dist/compressed-benchmark.js",
  "targets": {
    "main": {},
    "module": {},
    "compressed-benchmark": {
      "source": "./src/compressed-benchmark.ts",
      "outputFormat": "commonjs",
      "includeNodeModules": false,
      "optimize": true,
      "sourceMap": false,
      "isLibrary": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types.d.ts"
    }
  },
  "imports": {
    "#src": "./src"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@stylix/core": "^4.1.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.1.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lodash-es": "^4.17.21",
    "parcel": "^2.8.3",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
